{
  "queues": [
    {
      "name": "ai_tasks",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    }
  ],
  "exchanges": [
    {
      "name": "ai_events",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "ai_events",
      "vhost": "/",
      "destination": "ai_tasks",
      "destination_type": "queue",
      "routing_key": "task.*",
      "arguments": {}
    }
  ],
  "users": [
    {
      "name": "guest",
      "password_hash": "",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator"
    }
  ],
  "permissions": [
    {
      "user": "guest",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ]
}
